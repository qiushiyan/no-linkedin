{
  "manifest_version": 3,
  "name": "No Linkedin",
  "version": "1.0",
  "description": "Keeps you away from Linkedin, does not block /jobs",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "exclude_matches": ["*://*.linkedin.com/jobs/*"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["template.html"],
      "matches": ["*://*.linkedin.com/*"]
    }
  ]
}
